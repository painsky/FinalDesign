<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.classtable.pain.finaldesign">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/t_class"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service android:name=".utils.AlarmService"></service>
        <activity android:name=".ShowStudentActivity"/>
        <activity android:name=".WebActivity"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".AddClassActivity"/>
        <activity android:name=".BindClassActivity"/>
        <activity android:name=".EditClassActivity"/>
        <activity android:name=".SettingWeekActivity"/>
        <activity android:name=".SplashActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
    </application>

</manifest>
